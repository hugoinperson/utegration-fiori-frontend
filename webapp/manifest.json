{
	"_version": "1.1.0",
	"start_url": "start.html",
	"sap.app": {
		"_version": "1.1.0",
		"id": "utegration.fiori.frontend",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"ach": "sap",
		"resources": "resources.json",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.32.0"
		}
	},

	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},

	"sap.ui5": {
		"_version": "1.1.0",
		"dependencies": {
			"minUI5Version": "1.40.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"utegration.fiori.frontend.control": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "utegration.fiori.frontend.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{ "uri": "css/style.css" },
				{ "uri": "control/css/AnimateObject.css" },
				{ "uri": "control/css/AnimateStrokeText.css" },
				{ "uri": "control/css/FormatText.css" },
				{ "uri": "control/css/SlideNavContainer.css" },
				{ "uri": "control/css/Slide.css" }
			]
		},
		"rootView": {
			"viewName": "utegration.fiori.frontend.view.Index",
			"type": "XML"
		},
		"routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "async": true,
                "viewType": "XML",
                "viewPath": "utegration.fiori.frontend.view",
                "controlId": "indexApp",
                "controlAggregation": "pages",
                "transition": "slide"
            },
            "routes": [
            	{
            		"name": "launchpad",
                    "pattern": "",
                    "target": "launchpad"
                }, 
                {
                	"name": "topic-introduction",
                    "pattern": "introduction/:slidePageNum:",
                    "target": "topic-introduction"
                },
                {
                	"name": "topic-history",
                    "pattern": "history/:slidePageNum:",
                    "target": "topic-history"
                },
                {
                	"name": "topic-launchpad",
                    "pattern": "launchpad/:slidePageNum:",
                    "target": "topic-launchpad"
                },
                {
                	"name": "topic-fioriElement",
                    "pattern": "fioriElement/:slidePageNum:",
                    "target": "topic-fioriElement"
                }
            ],
            "targets": {
                "launchpad": {
                    "viewName": "Launchpad",
                    "transition": "fade"
                },
                "topic-introduction": {
                    "viewName": "TopicIntroduction"
                },
                "topic-history": {
                    "viewName": "TopicHistory"
                },
                "topic-launchpad": {
                    "viewName": "TopicLaunchpad"
                },
                "topic-fioriElement": {
                    "viewName": "TopicFioriElement"
                }
            }
	    }
	}
}