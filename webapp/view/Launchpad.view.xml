													<TileContent unit="Version" footer="1.42">
														<content>
															<ImageContent src="sap-icon://sap-ui5"/>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
										</html:div>
									</uxap:blocks>
								</uxap:ObjectPageSubSection>
							</uxap:subSections>
						</uxap:ObjectPageSection>
						<!--Work Space Group-->
						<uxap:ObjectPageSection title="Work Space">
							<uxap:subSections>
