<mvc:View 
	controllerName="utegration.fiori.frontend.controller.Launchpad" 
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:uxap="sap.uxap"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:u="sap.ui.unified"
	xmlns:l="sap.ui.layout"
	xmlns="sap.m">
	<Page 
		id="utg-fiori-hostPage" 
		class="utg-fiori-hostPage"
		enableScrolling="true">
		<headerContent>
			<OverflowToolbar
				id="utg-fiori-toolbar"
				class="utg-fiori-toolbar">
				<ImageContent
					class="utg-fiori-toolbar-img"
					src="img/avatar.png"
					description="Profile image"
					press="onUserAvatarPress">
				</ImageContent>
				<ImageContent
					class="utg-fiori-toolbar-img"
					src="img/favicon.png"
					description="Profile image"
					press="press">
				</ImageContent>
				<ToolbarSpacer/>
				<Button text="Home" type="Transparent" press="onHomePress"/>
				<ToolbarSpacer/>
				<core:Icon
					src="sap-icon://ui-notifications"
					class="utg-fiori-toolbar-notiCenterIcon"
					color="#031E48"
					press="onNotiCenterPress">
				</core:Icon>
			</OverflowToolbar>
		</headerContent>
		<content>
			<l:BlockLayout class="utg-fiori-launchpad-container">
					<l:BlockLayoutRow id="utg-fiori-launchpad-wrapper" class="utg-fiori-launchpad-wrapper" scrollable="true">
						<!--Me Area-->
						<l:BlockLayoutCell
							id="utg-fiori-launchpad-meArea"
							class="utg-fiori-launchpad-meArea"
							visible="false"
							width="70"
							title="Me area">
							<Text text="Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. At vero eos et accusam et justo duo dolores et ea rebum. Stet clita kasd gubergren, no sea takimata sanctus est Lorem ipsum dolor sit amet. Lorem ipsum dolor sit amet, consetetur sadipscing elitr."/>
						</l:BlockLayoutCell>
						<!--Main Area-->
						<l:BlockLayoutCell
							id="utg-fiori-launchpad-main"
							class="utg-fiori-launchpad-main"
							width="100">
							<uxap:ObjectPageLayout 
								id="utg-fiori-launchpad-main-objectPageLayout" 
								class="utg-fiori-launchpad-main-objectPageLayout"
								enableLazyLoading="false">
								<uxap:sections>
									<uxap:ObjectPageSection title="My Home">
										<uxap:subSections>
											<uxap:ObjectPageSubSection class="utg-fiori-launchpad-main-subSection">
												<uxap:blocks>
													<html:div>
														<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Status Loaded - no press event"
															subheader="Subheader" frameType="OneByOne">
															<tileContent>
																<TileContent unit="Unit" footer="Footer">
																	<content>
																		<ImageContent src="sap-icon://line-charts"/>
																	</content>
																</TileContent>
															</tileContent>
														</GenericTile>
														<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Status Loaded - with press event"
															subheader="Subheader" frameType="OneByOne" press="press">
															<tileContent>
																<TileContent unit="Unit" footer="Footer">
																	<content>
																		<ImageContent src="sap-icon://home-share"/>
																	</content>
																</TileContent>
															</tileContent>
														</GenericTile>
														<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Status Loading - no press event"
															subheader="Subheader" frameType="OneByOne" state="Loading">
															<tileContent>
																<TileContent unit="Unit" footer="Footer">
																	<content>
																		<NumericContent scale="M" value="2.1"
																			valueColor="Good" indicator="Up" />
																	</content>
																</TileContent>
															</tileContent>
														</GenericTile>
														<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Status Loading - with press event"
															subheader="Subheader" frameType="OneByOne" state="Loading" press="press">
															<tileContent>
																<TileContent unit="Unit" footer="Footer">
																	<content>
																		<NumericContent scale="M" value="1.96"
																			valueColor="Error" indicator="Down" />
																	</content>
																</TileContent>
															</tileContent>
														</GenericTile>
														<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Status Failed - no press event"
															subheader="Subheader" frameType="TwoByOne" state="Failed">
															<tileContent>
																<TileContent unit="Unit" footer="Footer">
																	<content>
																		<FeedContent
																			contentText="@@notify Great outcome of the Presentation today. The new functionality and the design was well received. Berlin, Tokyo, Rome, Budapest, New York, Munich, London"
																			subheader="Subheader" value="9">
																		</FeedContent>
																	</content>
																</TileContent>
															</tileContent>
														</GenericTile>
														<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Status Failed - with press event"
															subheader="Subheader" frameType="TwoByOne" state="Failed" press="press">
															<tileContent>
																<TileContent unit="Unit" footer="Footer">
																	<content>
																		<FeedContent
																			contentText="@@notify Great outcome of the Presentation today. The new functionality and the design was well received. Berlin, Tokyo, Rome, Budapest, New York, Munich, London"
																			subheader="Subheader" value="9">
																		</FeedContent>
																	</content>
																</TileContent>
															</tileContent>
														</GenericTile>
														<SlideTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout">
															<tiles>
																<GenericTile
																	backgroundImage="test-resources/sap/m/demokit/sample/GenericTileAsFeedTile/images/NewsImage1.png"
																	frameType="TwoByOne" state="Loading">
																	<tileContent>
																		<TileContent unit="Unit" footer="Footer">
																			<content>
																				<NewsContent
																					contentText="Status Loading"
																					subheader="Subheader">
																				</NewsContent>
																			</content>
																		</TileContent>
																	</tileContent>
																</GenericTile>
																<GenericTile
																	backgroundImage="test-resources/sap/m/demokit/sample/GenericTileAsFeedTile/images/NewsImage2.png"
																	frameType="TwoByOne" state="Loaded" press="press">
																	<tileContent>
																		<TileContent unit="Unit" footer="Footer">
																			<content>
																				<NewsContent
																					contentText="Status Loaded"
																					subheader="Subheader">
																				</NewsContent>
																			</content>
																		</TileContent>
																	</tileContent>
																</GenericTile>
															</tiles>
														</SlideTile>
														<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Status Disabled"
															subheader="Subheader" frameType="OneByOne" state="Disabled">
															<tileContent>
																<TileContent footer="Footer" unit="Unit">
																	<content>
																		<NumericContent value="3"
																			icon="sap-icon://travel-expense" />
																	</content>
																</TileContent>
															</tileContent>
														</GenericTile>
														<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Status Disabled - with press event"
															subheader="Subheader" frameType="OneByOne" state="Disabled" press="press">
															<tileContent>
																<TileContent footer="Footer" unit="Unit">
																	<content>
																		<NumericContent value="3"
																			icon="sap-icon://travel-expense" />
																	</content>
																</TileContent>
															</tileContent>
														</GenericTile>
													</html:div>
													
													
													
													
													
													
												</uxap:blocks>
											</uxap:ObjectPageSubSection>
										</uxap:subSections>
									</uxap:ObjectPageSection>
						 
									<uxap:ObjectPageSection title="Work Space">
										<uxap:subSections>
											<uxap:ObjectPageSubSection class="utg-fiori-launchpad-main-subSection">
												<uxap:blocks>
													<Text text="Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. At vero eos et accusam et justo duo dolores et ea rebum. Stet clita kasd gubergren, no sea takimata sanctus est Lorem ipsum dolor sit amet. Lorem ipsum dolor sit amet, consetetur sadipscing elitr."/>
												</uxap:blocks>
											</uxap:ObjectPageSubSection>
										</uxap:subSections>
									</uxap:ObjectPageSection>
						 
									<uxap:ObjectPageSection title="Internal Sales">
										<uxap:subSections>
											<uxap:ObjectPageSubSection class="utg-fiori-launchpad-main-subSection">
												<uxap:blocks>
													<Text text="Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. At vero eos et accusam et justo duo dolores et ea rebum. Stet clita kasd gubergren, no sea takimata sanctus est Lorem ipsum dolor sit amet. Lorem ipsum dolor sit amet, consetetur sadipscing elitr."/>
												</uxap:blocks>
											</uxap:ObjectPageSubSection>
										</uxap:subSections>
									</uxap:ObjectPageSection>
								</uxap:sections>
							</uxap:ObjectPageLayout>
							
							
							
							
							
							
							
							
							
							
							
						</l:BlockLayoutCell>
						<!--Notification Center-->
						<l:BlockLayoutCell
							id="utg-fiori-launchpad-notiCenter"
							class="utg-fiori-launchpad-notiCenter"
							visible="false"
							width="70"
							title="Notification Center">
							<ScrollContainer
								height="100%"
								width="100%"
								horizontal="false"
								vertical="true">
								<List id="notificationList" class="sapContrast sapContrastPlus">
								<NotificationListItem
										title="New order (#2525) With a very long title - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
										description="And with a very long description and long labels of the action buttons - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
										showCloseButton="true"
										datetime="1 hour"
										unread="true"
										priority="None"
										close="onItemClose"
										press="onListItemPress"
										authorName="Jean Doe"
										authorPicture="test-resources/sap/m/images/Woman_04.png">
									<buttons>
										<Button text="Accept All Requested Information" type="Accept" press="onAcceptPress"></Button>
										<Button text="Reject All Requested Information" type="Reject" press="onRejectPress"></Button>
									</buttons>
									</NotificationListItem>
				 
								<NotificationListItem
										title="New order (#2524), without action buttons"
										description="Short description"
										showCloseButton="true"
										datetime="3 days"
										unread="true"
										priority="High"
										close="onItemClose"
										press="onListItemPress"
										authorName="Office Notification"
										authorPicture="sap-icon://group">
								</NotificationListItem>
				 
								<NotificationListItem
										title="New order (#2523) With a long title - Lorem ipsum dolor sit amet, consectetur adipiscing elit."
										description="And short description"
										showCloseButton="false"
										unread="false"
										datetime="3 days"
										priority="High"
										close="onItemClose"
										press="onListItemPress"
										authorName="Patricia Clark"
										authorPicture="test-resources/sap/m/images/female_BaySu.jpg">
									<buttons>
										<Button text="Accept" type="Accept" press="onAcceptPress" icon="sap-icon://accept"></Button>
										<Button text="Reject" type="Reject" press="onRejectPress" icon="sap-icon://sys-cancel"></Button>
									</buttons>
								</NotificationListItem>
				 
								<NotificationListItem
										title="New order (#2522)"
										description="With a very long description - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
										showCloseButton="true"
										datetime="3 days"
										unread="true"
										priority="Medium"
										close="onItemClose"
										press="onListItemPress"
										authorName="John Smith"
										authorPicture="test-resources/sap/m/images/headerImg2.jpg">
								</NotificationListItem>
				 
								<NotificationListItem
										title="New order (#2521)"
										description="With a very long description and no action buttons below - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
										showCloseButton="true"
										datetime="3 days"
										unread="true"
										priority="Low"
										close="onItemClose"
										press="onListItemPress"
										authorName="John Smith"
										authorPicture="test-resources/sap/m/images/headerImg2.jpg">
								</NotificationListItem>
				 
								<NotificationListItem
										title="New order (#2525) With a very long title and truncation disabled by default! Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
										description="And a very long description and long labels of the action buttons - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
										showCloseButton="true"
										datetime="2 day"
										unread="false"
										priority="Low"
										close="onItemClose"
										press="onListItemPress"
										authorName="Jean Doe"
										authorPicture="test-resources/sap/m/images/Woman_04.png"
										truncate="false">
									<buttons>
										<Button text="Accept" type="Accept" press="onAcceptPress"></Button>
										<Button text="Reject" type="Reject" press="onRejectPress"></Button>
									</buttons>
								</NotificationListItem>
				 
								<NotificationListItem
										title="New order (#2525) With a very long title and with truncation enabled but 'Show More' hidden! Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
										description="And a very long description and long labels of the action buttons - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
										showCloseButton="true"
										datetime="2 day"
										unread="false"
										priority="Low"
										close="onItemClose"
										press="onListItemPress"
										authorName="Jean Doe"
										authorPicture="test-resources/sap/m/images/Woman_04.png"
										hideShowMoreButton="true"
										showButtons="false">
									<buttons>
										<Button text="Accept" type="Accept" press="onAcceptPress"></Button>
										<Button text="Reject" type="Reject" press="onRejectPress"></Button>
									</buttons>
								</NotificationListItem>
				 
								<NotificationListItem
										title="New order (#2523) With a long title without description - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Lorem ipsum dolor sit amet"
										showCloseButton="false"
										unread="false"
										datetime="3 days"
										priority="High"
										close="onItemClose"
										press="onListItemPress"
										authorName="Patricia Clark"
										authorPicture="test-resources/sap/m/images/female_BaySu.jpg">
									<buttons>
										<Button text="Accept" type="Accept" press="onAcceptPress" icon="sap-icon://accept"></Button>
										<Button text="Reject" type="Reject" press="onRejectPress" icon="sap-icon://sys-cancel"></Button>
									</buttons>
								</NotificationListItem>
				                <NotificationListItem
				                        title="New order (#2523) With a long title without description"
				                        showCloseButton="true"
				                        unread="false"
				                        datetime="3 days"
				                        priority="High"
				                        close="onItemClose"
				                        press="onListItemPress">
				                </NotificationListItem>
							</List>
							</ScrollContainer>
						</l:BlockLayoutCell>
					</l:BlockLayoutRow>
				</l:BlockLayout>

			
			
			
			
			
		</content>
	</Page>
</mvc:View>