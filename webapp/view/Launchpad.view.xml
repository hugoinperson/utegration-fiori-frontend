									<uxap:blocks>
										<html:div class="utg-fiori-launchpad-main-tileContainer">
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="Status Loaded - no press event"
												subheader="Subheader" frameType="OneByOne">
												<tileContent>
													<TileContent unit="Unit" footer="Footer">
														<content>
															<ImageContent src="sap-icon://line-charts"/>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="Status Loaded - with press event"
