<mvc:View
    controllerName="utegration.fiori.frontend.controller.Launchpad"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:uxap="sap.uxap"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns="sap.m">

    <l:BlockLayout class="utg-fiori-launchpad-container">
		<l:BlockLayoutRow id="utg-fiori-launchpad-wrapper" class="utg-fiori-launchpad-wrapper" scrollable="true">
			<!--Me Area-->
			<l:BlockLayoutCell
				id="utg-fiori-launchpad-meArea"
				class="utg-fiori-launchpad-meArea"
				visible="false"
				width="70">
				<Text text="Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. At vero eos et accusam et justo duo dolores et ea rebum. Stet clita kasd gubergren, no sea takimata sanctus est Lorem ipsum dolor sit amet. Lorem ipsum dolor sit amet, consetetur sadipscing elitr."/>
			</l:BlockLayoutCell>
			<!--Main Area-->
			<l:BlockLayoutCell
				id="utg-fiori-launchpad-main"
				class="utg-fiori-launchpad-main"
				width="100">
				<uxap:ObjectPageLayout 
					id="utg-fiori-launchpad-main-objectPageLayout" 
					class="utg-fiori-launchpad-main-objectPageLayout"
					enableLazyLoading="false">
					<uxap:sections>
						<!--Introduction Group-->
						<uxap:ObjectPageSection title="Introduction">
							<uxap:subSections>
								<uxap:ObjectPageSubSection class="utg-fiori-launchpad-main-subSection">
									<uxap:blocks>
										<html:div class="utg-fiori-launchpad-main-tileContainer">
											<!--Introduction-->
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="Introduction"
												subheader="Fiori front-end programming" frameType="OneByOne" app:tileName="topic-introduction" press="onLaunchpadTilePress">
												<tileContent>
													<TileContent>
														<content>
															<ImageContent src="sap-icon://education"/>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
											<!--Firori History and Evolution-->
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="History"
												subheader="Subheader" frameType="OneByOne" app:tileName="topic-history" press="onLaunchpadTilePress">
												<tileContent>
													<TileContent>
														<content>
															<ImageContent src="sap-icon://sap-box"/>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
										</html:div>
									</uxap:blocks>
								</uxap:ObjectPageSubSection>
							</uxap:subSections>
						</uxap:ObjectPageSection>
						<!--Fiori 2.0 Group-->
						<uxap:ObjectPageSection title="Fiori 2.0">
							<uxap:subSections>
								<uxap:ObjectPageSubSection class="utg-fiori-launchpad-main-subSection">
									<uxap:blocks>
										<html:div class="utg-fiori-launchpad-main-tileContainer">
											<!--New Launchpad-->
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="New Launchpad"
												subheader="Single entry point to access all apps" frameType="OneByOne" app:tileName="topic-launchpad" press="onLaunchpadTilePress">
												<tileContent>
													<TileContent unit="20" footer="slides">
														<content>
															<ImageContent src="sap-icon://business-one"/>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
											<!--Fiori Elements-->
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="Fiori Elements"
												subheader="Generating UIs at runtime" frameType="OneByOne" app:tileName="topic-ui5" press="onLaunchpadTilePress">
												<tileContent>
													<TileContent unit="20" footer="slides">
														<content>
															<ImageContent src="sap-icon://business-one"/>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
											<!--Fiori for iOS-->
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="Fiori for iOS"
												subheader="Fiori desgin guideline for iOS" frameType="OneByOne" app:tileName="topic-ui5" press="onLaunchpadTilePress">
												<tileContent>
													<TileContent unit="20" footer="slides">
														<content>
															<ImageContent src="sap-icon://business-one"/>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
											<!--Fiori Frontend Architecture-->
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="Fiori Frontend Architecture"
												subheader="Fiori desgin guideline for iOS" frameType="OneByOne" app:tileName="topic-ui5" press="onLaunchpadTilePress">
												<tileContent>
													<TileContent unit="20" footer="slides">
														<content>
															<ImageContent src="sap-icon://business-one"/>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
											<!--Upgrading to Fiori 2.0-->
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="Adopting to Fiori 2.0"
												subheader="Fiori desgin guideline for iOS" frameType="OneByOne" app:tileName="topic-ui5" press="onLaunchpadTilePress">
												<tileContent>
													<TileContent unit="20" footer="slides">
														<content>
															<ImageContent src="sap-icon://business-one"/>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
										</html:div>
									</uxap:blocks>
								</uxap:ObjectPageSubSection>
							</uxap:subSections>
						</uxap:ObjectPageSection>
						<!--Development Group-->
						<uxap:ObjectPageSection title="Development">
							<uxap:subSections>
								<uxap:ObjectPageSubSection class="utg-fiori-launchpad-main-subSection">
									<uxap:blocks>
										<html:div class="utg-fiori-launchpad-main-tileContainer">
											<!--UI5-->
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="SAP UI5"
												subheader="Version 1.42" frameType="OneByOne" app:tileName="topic-ui5" press="onLaunchpadTilePress">
												<tileContent>
													<TileContent unit="20" footer="slides">
														<content>
															<ImageContent src="sap-icon://sap-ui5"/>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
											<!--OData-->
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="OData"
												subheader="Version 4.0" frameType="OneByOne" press="press">
												<tileContent>
													<TileContent unit="30" footer="slides">
														<content>
															<ImageContent src="sap-icon://home-share"/>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
											<!--SAP Web IDE-->
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="SAP Web IDE"
												subheader="IDE for UI5 apps" frameType="TwoByOne">
												<tileContent>
													<TileContent unit="15" footer="slides">
														<content>
															<FeedContent
																contentText="SAP Web IDE is a powerful, extensible, web-based integrated development tool that simplifies end-to-end application development for SAP Fiori and SAPUI5 apps."
																subheader="" value="">
															</FeedContent>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
											<!--Git-->
											<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop utg-fiori-launchpad-tileLayout" header="Git"
												subheader="Version control system" frameType="OneByOne" press="press">
												<tileContent>
													<TileContent unit="30" footer="slides">
														<content>
															<ImageContent src="sap-icon://home-share"/>
														</content>
													</TileContent>
												</tileContent>
											</GenericTile>
										</html:div>
									</uxap:blocks>
								</uxap:ObjectPageSubSection>
							</uxap:subSections>
						</uxap:ObjectPageSection>
					</uxap:sections>
				</uxap:ObjectPageLayout>
			</l:BlockLayoutCell>
			<!--Notification Center-->
			<l:BlockLayoutCell
				id="utg-fiori-launchpad-notiCenter"
				class="utg-fiori-launchpad-notiCenter"
				visible="false"
				width="70">
				<ScrollContainer
					height="100%"
					width="100%"
					horizontal="false"
					vertical="true">
					<List id="notificationList" class="sapContrast sapContrastPlus">
					<NotificationListItem
							title="New order (#2525) With a very long title - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
							description="And with a very long description and long labels of the action buttons - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
							showCloseButton="true"
							datetime="1 hour"
							unread="true"
							priority="None"
							close="onItemClose"
							press="onListItemPress"
							authorName="Jean Doe"
							authorPicture="test-resources/sap/m/images/Woman_04.png">
						<buttons>
							<Button text="Accept All Requested Information" type="Accept" press="onAcceptPress"></Button>
							<Button text="Reject All Requested Information" type="Reject" press="onRejectPress"></Button>
						</buttons>
						</NotificationListItem>
	 
					<NotificationListItem
							title="New order (#2524), without action buttons"
							description="Short description"
							showCloseButton="true"
							datetime="3 days"
							unread="true"
							priority="High"
							close="onItemClose"
							press="onListItemPress"
							authorName="Office Notification"
							authorPicture="sap-icon://group">
					</NotificationListItem>
	 
					<NotificationListItem
							title="New order (#2523) With a long title - Lorem ipsum dolor sit amet, consectetur adipiscing elit."
							description="And short description"
							showCloseButton="false"
							unread="false"
							datetime="3 days"
							priority="High"
							close="onItemClose"
							press="onListItemPress"
							authorName="Patricia Clark"
							authorPicture="test-resources/sap/m/images/female_BaySu.jpg">
						<buttons>
							<Button text="Accept" type="Accept" press="onAcceptPress" icon="sap-icon://accept"></Button>
							<Button text="Reject" type="Reject" press="onRejectPress" icon="sap-icon://sys-cancel"></Button>
						</buttons>
					</NotificationListItem>
	 
					<NotificationListItem
							title="New order (#2522)"
							description="With a very long description - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
							showCloseButton="true"
							datetime="3 days"
							unread="true"
							priority="Medium"
							close="onItemClose"
							press="onListItemPress"
							authorName="John Smith"
							authorPicture="test-resources/sap/m/images/headerImg2.jpg">
					</NotificationListItem>
	 
					<NotificationListItem
							title="New order (#2521)"
							description="With a very long description and no action buttons below - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
							showCloseButton="true"
							datetime="3 days"
							unread="true"
							priority="Low"
							close="onItemClose"
							press="onListItemPress"
							authorName="John Smith"
							authorPicture="test-resources/sap/m/images/headerImg2.jpg">
					</NotificationListItem>
	 
					<NotificationListItem
							title="New order (#2525) With a very long title and truncation disabled by default! Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
							description="And a very long description and long labels of the action buttons - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
							showCloseButton="true"
							datetime="2 day"
							unread="false"
							priority="Low"
							close="onItemClose"
							press="onListItemPress"
							authorName="Jean Doe"
							authorPicture="test-resources/sap/m/images/Woman_04.png"
							truncate="false">
						<buttons>
							<Button text="Accept" type="Accept" press="onAcceptPress"></Button>
							<Button text="Reject" type="Reject" press="onRejectPress"></Button>
						</buttons>
					</NotificationListItem>
	 
					<NotificationListItem
							title="New order (#2525) With a very long title and with truncation enabled but 'Show More' hidden! Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
							description="And a very long description and long labels of the action buttons - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Praesent feugiat, turpis vel scelerisque pharetra, tellus odio vehicula dolor, nec elementum lectus turpis at nunc."
							showCloseButton="true"
							datetime="2 day"
							unread="false"
							priority="Low"
							close="onItemClose"
							press="onListItemPress"
							authorName="Jean Doe"
							authorPicture="test-resources/sap/m/images/Woman_04.png"
							hideShowMoreButton="true"
							showButtons="false">
						<buttons>
							<Button text="Accept" type="Accept" press="onAcceptPress"></Button>
							<Button text="Reject" type="Reject" press="onRejectPress"></Button>
						</buttons>
					</NotificationListItem>
	 
					<NotificationListItem
							title="New order (#2523) With a long title without description - Lorem ipsum dolor sit amet, consectetur adipiscing elit. Lorem ipsum dolor sit amet"
							showCloseButton="false"
							unread="false"
							datetime="3 days"
							priority="High"
							close="onItemClose"
							press="onListItemPress"
							authorName="Patricia Clark"
							authorPicture="test-resources/sap/m/images/female_BaySu.jpg">
						<buttons>
							<Button text="Accept" type="Accept" press="onAcceptPress" icon="sap-icon://accept"></Button>
							<Button text="Reject" type="Reject" press="onRejectPress" icon="sap-icon://sys-cancel"></Button>
						</buttons>
					</NotificationListItem>
	                <NotificationListItem
	                        title="New order (#2523) With a long title without description"
	                        showCloseButton="true"
	                        unread="false"
	                        datetime="3 days"
	                        priority="High"
	                        close="onItemClose"
	                        press="onListItemPress">
	                </NotificationListItem>
				</List>
				</ScrollContainer>
			</l:BlockLayoutCell>
		</l:BlockLayoutRow>
	</l:BlockLayout>

</mvc:View>