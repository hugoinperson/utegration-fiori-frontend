{
	"_version": "1.5.0",
	"start_url": "start.html",
	"sap.app": {
		"_version": "1.1.0",
		"id": "utegration.fiori.frontend.component.fioriElement",
		"type": "component",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"NorthwindModel": {
				"uri": "/northwind/northwind/northwind.svc/",
				"type": "OData",
				"settings": {
					"annotations": ["localAnnotations_1"],
					"localUri": "localService/NorthwindModel/metadata.xml"
				}
			},
			"localAnnotations_1": {
				"uri": "annotations/localAnnotations_1.xml",
				"type": "ODataAnnotation",
				"settings": {
					"localUri": "annotations/localAnnotations_1.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "OVP.cardtemplate",
			"version": "0.0.0"
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"dependencies": {
			"minUI5Version": "1.32.0",
			"libs": {
				"sap.ovp": {
					"minVersion": "1.32.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "utegration.fiori.frontend.component.fioriElement.i18n.i18n"
				}
			},
			"NorthwindModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneTime",
					"defaultCountMode": "Request"
				},
				"dataSource": "NorthwindModel",
				"preload": true
			}
		},
		"routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "async": true,
                "viewType": "XML",
                "viewPath": "utegration.fiori.frontend.component.fioriElement.view",
                "controlId": "indexApp",
                "controlAggregation": "pages",
                "transition": "slide"
            },
            "routes": [
            	{
            		"name": "main",
                    "pattern": "fioriElement/:slidePageNum:",
                    "target": "main"
                }
            ],
            "targets": {
                "main": {
                    "viewName": "Main"
                }
            }
        }
	},
	"sap.ovp": {
		"_version": "1.1.0",
		"globalFilterModel": "NorthwindModel",
		"globalFilterEntityType": "",
		"cards": {
			"utegration.fiori.frontend_card01": {
				"model": "NorthwindModel",
				"template": "sap.ovp.cards.list",
				"settings": {
					"title": "Products",
					"sortBy": "ProductName",
					"sortOrder": "descending",
					"listFlavor": "bar",
					"entitySet": "Products"
				}
			}
		}
	}
}