<mvc:View
	controllerName="utegration.fiori.frontend.component.iOS.controller.BasicSlide"
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:utg="utegration.fiori.frontend.control"
    xmlns:m="sap.m"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    displayBlock="true"
    height="100%">
    
    <!--Base-->
	<utg:AnimateBox
		height="100%"
		direction="Column"
		alignItems="Center"
		justifyContent="Center">
		
		<utg:AnimateBox
			width="60%"
			direction="Column"
			alignItems="Center"
			justifyContent="Center">
			<utg:FormatText
				level="H2">
				<m:Text text="SAP is bringing Fiori to iOS with a"/>
				<utg:AnimateStrokeText order="1" text="new suite of apps" />
				<m:Text text="and an"/>
				<utg:AnimateStrokeText order="2" text="SDK" />
				<m:Text text="for developers."/>
			</utg:FormatText>
		</utg:AnimateBox>
	
	</utg:AnimateBox>

</mvc:View>