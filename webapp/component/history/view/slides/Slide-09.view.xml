<mvc:View
	controllerName="utegration.fiori.frontend.component.history.controller.BasicSlide"
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:u="sap.uxap"
    xmlns:utg="utegration.fiori.frontend.control"
    xmlns:m="sap.m"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    displayBlock="true"
    height="100%">
    
    <!--Base-->
	<utg:AnimateBox
		height="100%"
		direction="Column"
		alignItems="Center"
		justifyContent="Center">
		
		<utg:AnimateBox
			width="80%"
			direction="Column"
			alignItems="Center"
			justifyContent="Center">
			
			<utg:FormatText level="H2" class="sapUiMediumMarginBottom">
				<m:Text text="SAP Fiori 2.0 (2016)"/>
			</utg:FormatText>
			
			<u:ObjectPageLayout
                id="ObjectPageLayout"
                height="50rem"
                enableLazyLoading="false"
                showTitleInHeaderContent="true">

                <!-- Header Title -->
                <u:headerTitle>
                    <u:ObjectPageHeader
				        class="ccuxObjectPageHeader"
				        xmlns:u="sap.uxap"
				        xmlns:m="sap.m"
				        headerDesign="Light"
				        showTitleSelector="false"
				        objectImageShape="Circle"
				        objectImageAlt="{oBpModel>/FirstName} {oBpModel>/LastName}"
				        objectImageURI="./img/jimmy.jpeg"
				        objectTitle="Jimmy Leong"
				        objectSubtitle="{oBpModel>/CustomerSegmentation}"
				        isObjectIconAlwaysVisible="false"
				        isObjectTitleAlwaysVisible="false"
				        isObjectSubtitleAlwaysVisible="false">
				        <u:breadCrumbsLinks>
							<m:Link text="XDDDDD" press="onBpNoPress"/>
							<m:Link text="XDDFFFF" press="onCaNoPress"/>
						</u:breadCrumbsLinks>
				        <u:actions>
				            <u:ObjectPageHeaderActionButton text="Customer Journey" hideIcon="true" hideText="false" type="Accept" press="toggleCustomerJourney"/>
				        </u:actions>
				    </u:ObjectPageHeader>
                </u:headerTitle>

                <!-- Header Content -->
                <u:headerContent>
                    <l:VerticalLayout>
				        <m:ObjectStatus state="Success" icon="sap-icon://loan" text="Deposit"/>
				        <m:FlexBox>
				            <m:items>
				                <m:Button class="sapUiTinyMarginEnd" visible="{oBpModel>/GroupWater}" text="Water" type="Accept"/>
				                <m:Button class="sapUiTinyMarginEnd" visible="{oBpModel>/GroupGas}" text="Gas" type="Accept"/>
				                <m:Button class="sapUiTinyMarginEnd" visible="{oBpModel>/GroupElectricity}" text="Electricity" type="Accept"/>
				                <m:Button class="sapUiTinyMarginEnd" visible="{oBpModel>/GroupTrash}" text="Trash" type="Accept"/>
				                <m:Button class="sapUiTinyMarginEnd" visible="{oBpModel>/GroupSewer}" text="Sewer" type="Accept"/>
				            </m:items>
				        </m:FlexBox>
				    </l:VerticalLayout>
				
				    <l:VerticalLayout
				        xmlns:l="sap.ui.layout"
				        xmlns:m="sap.m">
				        <m:ObjectStatus title="Location" text="{oCoModel>/City}, {oCoModel>/State}"/>
				        <m:ObjectStatus title="Weather" text="{oCoModel>/LocalTemperature}"/>
				        <m:ObjectStatus title="Service length" text="{oBpModel>/ServiceLength}"/>
				        <m:ObjectStatus title="Contract account name" text="{oBpModel>/FirstName} {oBpModel>/LastName}"/>
				    </l:VerticalLayout>
				
				    <l:VerticalLayout
				        xmlns:l="sap.ui.layout"
				        xmlns:m="sap.m">
				        <m:Label id="ccuxCaComboBoxLabel" text="Contract Account"/>
				        <m:ComboBox
				            id="ccuxCaComboBox"
				            class="ccuxHeaderComboBox"
				            selectionChange="onCcuxCaComboBoxChange"
				            showSecondaryValues="true"
				            items="{
				                path: 'oCaMenuModel>/'
				            }">
				            <core:ListItem
				                key="{oCaMenuModel>CaNo}"
				                text="{oCaMenuModel>CaNo}"
				                additionalText="{= ${oCaMenuModel>PoBox} !== '' ? 'PoBox' : '' } {oCaMenuModel>PoBox} {oCaMenuModel>HouseNo} {oCaMenuModel>City} {= ${oCaMenuModel>UnitNo} !== '' ? '#' : '' } {oCaMenuModel>UnitNo}, {oCaMenuModel>Street}, {oCaMenuModel>City}, {oCaMenuModel>State} {oCaMenuModel>ZipCode}"/>
				        </m:ComboBox>
				        <m:Label id="ccuxCoComboBoxLabel" text="Contract"/>
				        <m:ComboBox
				            id="ccuxCoComboBox"
				            class="ccuxHeaderComboBox"
				            selectionChange="onCcuxCoComboBoxChange"
				            showSecondaryValues= "true"
				            items="{
				                path: 'oCoMenuModel>/'
				            }">
				            <core:ListItem
				                key="{oCoMenuModel>CoNo}"
				                text="{oCoMenuModel>CoNo}"
				                additionalText="{= ${oCoMenuModel>PoBox} !== '' ? 'PoBox' : '' } {oCoMenuModel>PoBox} {oCoMenuModel>HouseNo} {oCoMenuModel>City} {= ${oCoMenuModel>UnitNo} !== '' ? '#' : '' } {oCoMenuModel>UnitNo}, {oCoMenuModel>Street}, {oCoMenuModel>City}, {oCoMenuModel>State} {oCoMenuModel>ZipCode}"/>
				        </m:ComboBox>
				    </l:VerticalLayout>
                </u:headerContent>

                <!-- Sections -->
                <u:sections>

                    <!-- Usage Graph -->
                    <u:ObjectPageSection id="ccuxHighBillNonCustomerDriven-PageSection" class="ccuxHighBill-PageSection" title="Usage Chart">
                        <u:subSections>
                            <u:ObjectPageSubSection id="ccuxHighBillNonCustomerDriven-PageSubSection" class="ccuxHighBill-PageSubSection">
                                <u:blocks>
                                    <!-- Block Content -->
                                    <m:Text text="sdfsdfsdf" />

                                </u:blocks>
                            </u:ObjectPageSubSection>
                        </u:subSections>
                    </u:ObjectPageSection>

                    <!-- Usage Table -->
                    <u:ObjectPageSection class="ccuxUsage-PageSection" title="Usage Table">
                        <u:subSections>
                            <u:ObjectPageSubSection class="ccuxUsage-PageSubSection">
                                <u:blocks>
                                    <!-- Block Content -->
									<m:Text text="sdfsdfsdf" />

                                </u:blocks>
                            </u:ObjectPageSubSection>
                        </u:subSections>
                    </u:ObjectPageSection>
                    
                    <!-- Usage Table -->
                    <u:ObjectPageSection class="ccuxUsage-PageSection" title="Usage Table">
                        <u:subSections>
                            <u:ObjectPageSubSection class="ccuxUsage-PageSubSection">
                                <u:blocks>
                                    <!-- Block Content -->
									<m:Text text="sdfsdfsdf" />

                                </u:blocks>
                            </u:ObjectPageSubSection>
                        </u:subSections>
                    </u:ObjectPageSection>
                    
                    <!-- Usage Table -->
                    <u:ObjectPageSection class="ccuxUsage-PageSection" title="Usage Table">
                        <u:subSections>
                            <u:ObjectPageSubSection class="ccuxUsage-PageSubSection">
                                <u:blocks>
                                    <!-- Block Content -->
									<m:Text text="sdfsdfsdf" />

                                </u:blocks>
                            </u:ObjectPageSubSection>
                        </u:subSections>
                    </u:ObjectPageSection>
                    
                	<!-- Usage Table -->
                    <u:ObjectPageSection class="ccuxUsage-PageSection" title="Usage Table">
                        <u:subSections>
                            <u:ObjectPageSubSection class="ccuxUsage-PageSubSection">
                                <u:blocks>
                                    <!-- Block Content -->
									<m:Text text="sdfsdfsdf" />

                                </u:blocks>
                            </u:ObjectPageSubSection>
                        </u:subSections>
                    </u:ObjectPageSection>
                    
                    <!-- Usage Table -->
                    <u:ObjectPageSection class="ccuxUsage-PageSection" title="Usage Table">
                        <u:subSections>
                            <u:ObjectPageSubSection class="ccuxUsage-PageSubSection">
                                <u:blocks>
                                    <!-- Block Content -->
									<m:Text text="sdfsdfsdf" />

                                </u:blocks>
                            </u:ObjectPageSubSection>
                        </u:subSections>
                    </u:ObjectPageSection>
                    
                    <!-- Usage Table -->
                    <u:ObjectPageSection class="ccuxUsage-PageSection" title="Usage Table">
                        <u:subSections>
                            <u:ObjectPageSubSection class="ccuxUsage-PageSubSection">
                                <u:blocks>
                                    <!-- Block Content -->
									<m:Text text="sdfsdfsdf" />

                                </u:blocks>
                            </u:ObjectPageSubSection>
                        </u:subSections>
                    </u:ObjectPageSection>
                    
                    <!-- Usage Table -->
                    <u:ObjectPageSection class="ccuxUsage-PageSection" title="Usage Table">
                        <u:subSections>
                            <u:ObjectPageSubSection class="ccuxUsage-PageSubSection">
                                <u:blocks>
                                    <!-- Block Content -->
									<m:Text text="sdfsdfsdf" />

                                </u:blocks>
                            </u:ObjectPageSubSection>
                        </u:subSections>
                    </u:ObjectPageSection>

                </u:sections>
            </u:ObjectPageLayout>
			
			
			
			
			
		</utg:AnimateBox>
	
	</utg:AnimateBox>

</mvc:View>